  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "esbuild": "^0.12.17"
  },
  "devDependencies": {
    "@types/node": "^16.4.10",
    "eslint": "^7.32.0",
    "gts": "^3.1.0",
    "jest": "^27.0.6",
    "typescript": "^4.3.5"
  }
